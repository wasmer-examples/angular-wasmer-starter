{
  "name": "angular-wasmer-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run pages:build && wrangler pages dev dist/winterjs --compatibility-date=2024-02-08 --experimental-local",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:angular-wasmer-starter": "node dist/angular-wasmer-starter/server/server.mjs",
    "process": "node ./tools/copy-files.mjs && node ./tools/alter-polyfills.mjs",
    "pages:build": "ng build && npm run process",
    "deploy": "npm run pages:build && wrangler pages deploy dist/winterjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/platform-server": "^17.2.0",
    "@angular/router": "^17.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "^17.2.0",
    "@angular/compiler-cli": "^17.2.0",
    "@miniflare/tre": "^3.0.0-next.13",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.2",
    "wrangler": "^0.0.0-ef064279"
  }
}